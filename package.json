{
  "name": "dd-timelapse",
  "version": "1.0.0",
  "description": "A screen shotting application to timelapse the development process.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:win": "electron-builder build --win --publish never",
    "build:linux": "electron-builder build --linux --publish never"
  },
  "build": {
    "appId": "com.devduo.timelapser",
    "productName": "Timelapser",
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "snap"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "dotenv": "^8.2.0",
    "electron": "^7.0.0",
    "electron-builder": "^22.1.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "aws-sdk": "^2.558.0",
    "crypto": "^1.0.1",
    "electron-store": "^5.0.0",
    "fs": "0.0.1-security",
    "imagemin": "^7.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-pngquant": "^8.0.0",
    "os": "^0.1.1",
    "path": "^0.12.7"
  }
}
